local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("kirware", Midnight)
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Main")
Section:NewButton("Animation", "ButtonInfo", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/akanezscripts/ezware/main/animationchanger"))()
print("Animation")
end)
Section:NewButton("SilentAim", "ButtonInfo", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/akanezscripts/kizware/main/silentaim"))()
print("SIlent")
end)
Section:NewButton("EvoHub", "ButtonInfo", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/akanezscripts/evo/main/evohub"))()
print("Fly")
end)
Section:NewButton("Spammer", "ButtonInfo", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/akanezscripts/kizware-misc/main/misc"))()
print("Kizware on top")
end)
Section:NewButton("Fly", "ButtonInfo", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/akanezscripts/kizware-silent/main/silent"))()
print("Fly")
end)
Section:NewKeybind("Keybind", "KeybindInfo", Enum.KeyCode.V, function()
	Library:ToggleUI()
end)
